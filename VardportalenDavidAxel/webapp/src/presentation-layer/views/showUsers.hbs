{{#if errors.length}}
<h3 class="title is-3"> errors:</h3>

<ul>
    {{#each errors}}
    <li>{{this}}</li>
    {{/each}}
</ul>
{{/if}}

{{#if users.length}}
<ul>
    {{#each users}}
    <li>  
        <h5 class="title is-5"> {{this.firstName}} {{this.lastName}}</h5>
    </li>
    {{/each}}
</ul>

{{/if}}